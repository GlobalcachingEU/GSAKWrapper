#*******************************************
# MacVersion = 0.5
# MacDescription = Executes or installs the GSAKwrapper
# MacAuthor = Mars Express
# MacFileName = GSAKWrapper.gsk
# MacUrl =
#*******************************************
#  GSAKWrapper macro  Version History
# V0.3 7-11-15   First release
# V0.4 7-11-15   Cosmetic changes
# V0.5 25-11-15 Enabled update check, changed install check, changed Label2 visibility, Published

UPDATECHECK Frequency=10 #check for macro update on gsak forum
VERCHECK Version=8.2.0.00 

Option Explicit=Yes

Declare Var=$execParam Type=String
Declare Var=$Form Type=String
Declare Var=$FormExit Type=String
Declare Var=$GlobalcachingEuLogo Type=String
Declare Var=$GSAKWrapperPath Type=String
Declare Var=$infoText Type=String
Declare Var=$Installed Type=Boolean
Declare Var=$result Type=String
Declare Var=$settingsdb Type=String


$settingsdb = "C:\programdata\GSAKWrapper\settings.db3"
$execParam="-d=" + Quote($_CurrentDatabase)
#$execParam = $execParam + " -f=" + Quote("flow_xyz")  #to be patched by GSAKwrapper.exe


While True # Main loop

	$Installed = False  #install check
	If FileExists($settingsdb)
		$result = Sqlite("open", $settingsdb)
		$_sql = "select  item_value from Settings where item_name='ApplicationPath'"  # get scopename
		$GSAKWrapperPath = Sqlite("sql", $_sql)
		$Installed = FileExists($GSAKWrapperPath)
	endif

	if $Installed = True
		$Form = EditForm($Form,"Label2","Visible","No")
	else 
		$Form = EditForm($Form,"Label2","Visible","Yes")
	endif

	if $_StartShift = False
		if $Installed = True
		  RUNPGM PGM=$GSAKWrapperPath  parms=$execParam Wait=Yes Hide=No 
		  Return #exit wrapper means exit macro.
		endif
	endif
	
	#Startshift or GSAKwrapper not found, so display form
		$FormExit = Form($form,"")
		BeginCase
			Case ($FormExit="SystemExit") OR ($FormExit = "Button_Cancel")
				Return
			Case $FormExit="Button_Wiki"
				WEB URL="https://github.com/GlobalcachingEU/GSAKWrapper/wiki"
			Case $FormExit="Button_Download"
				WEB URL="https://github.com/GlobalcachingEU/GSAKWrapper/releases"
			Case ($FormExit="GlobalcachingEuLogo")
				WEB URL="http://www.globalcaching.eu"
		EndCase
		
EndWhile
	
	
###################

<data> VarName=$infoText 
GSAK Wrapper is a GSAK database filter/export utility that can perform actions on one or more GSAK-databases without the use of user written macro's. 
<enddata>

